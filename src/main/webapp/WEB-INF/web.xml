<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>StudentServlet</servlet-name>
        <servlet-class>com.huqingyong.www.contoller.StudentServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StudentServlet</servlet-name>
        <url-pattern>/studentServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ManagerServlet</servlet-name>
        <servlet-class>com.huqingyong.www.contoller.ManagerServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ManagerServlet</servlet-name>
        <url-pattern>/managerServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>SponsorServlet</servlet-name>
        <servlet-class>com.huqingyong.www.contoller.SponsorServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SponsorServlet</servlet-name>
        <url-pattern>/sponsorServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ActivityServlet</servlet-name>
        <servlet-class>com.huqingyong.www.contoller.ActivityServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ActivityServlet</servlet-name>
        <url-pattern>/activityServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AnnouncementServlet</servlet-name>
        <servlet-class>com.huqingyong.www.contoller.AnnouncementServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AnnouncementServlet</servlet-name>
        <url-pattern>/announcementServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>FileServlet</servlet-name>
        <servlet-class>com.huqingyong.www.contoller.FileServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FileServlet</servlet-name>
        <url-pattern>/fileServlet</url-pattern>
    </servlet-mapping>

</web-app>